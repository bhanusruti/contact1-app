<table class="table table-striped purple_hippo">
  <tr>
    <th>First Name</th>
     <th>Last Name</th>
    <th>Phone Number</th>
    <th>Created_at</th>
    <th>Updated-at</th>
  </tr>
  <% @contacts.each do |contact| %>
    <tr>
      <td><%= contact.first_name %></td>
      <td><%= contact.last_name %></td>
      <td><%= contact.phone_number %></td>
      <td><%= contact.created_at %></td>
      <td><%= contact.friendly_updated_at %></td>
      <td><%= contact.phone_num %></td>
    </tr>


  <% end %>

</table>